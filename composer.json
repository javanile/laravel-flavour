{
  "name": "javanile/laravel-flavour",
  "description": "Laravel helper functions extracted as standalone utilities for any PHP project",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "helpers", "utilities", "php", "support", "collections"],
  "homepage": "https://github.com/javanile/laravel-flavour",
  "support": {
    "issues": "https://github.com/javanile/laravel-flavour/issues",
    "source": "https://github.com/javanile/laravel-flavour"
  },
  "authors": [
    {
      "name": "Francesco Bianco",
      "email": "bianco@javanile.org",
      "homepage": "https://javanile.org",
      "role": "Developer"
    }
  ],
  "maintainers": [
    {
      "name": "Francesco Bianco",
      "email": "bianco@javanile.org"
    }
  ],
  "funding": [
    {
      "type": "custom",
      "url": "https://javanile.org/funding"
    }
  ],
  "suggest": {
    "modernization": "Interested in modernizing your legacy app with Docker or introducing modern frameworks? Contact me at bianco@javanile.org for consultation and development services."
  },
  "require-dev": {
    "pestphp/pest": "^4.1"
  },
  "autoload": {
    "files": [
      "src/Illuminate/Support/helpers.php",
      "src/Illuminate/Collections/helpers.php"
    ],
    "psr-4": {
      "Illuminate\\": "src/Illuminate/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "phpoption/phpoption": "^1.9",
    "vlucas/phpdotenv": "^5.6"
  }
}
